      <Label fx:id="titleLabel" layoutX="85.0" layoutY="79.0" styleClass="title" text="Title" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="40.0" />
      <TextFlow fx:id="contentFlow" layoutX="50.0" layoutY="70.0" prefHeight="106.0" prefWidth="300.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="80.0" />
